    <img class="mascot" src="https://i.postimg.cc/mZCrhhny/Pngtree-cryptocurrency-miner-guy-5315592.png" alt="">
</section>
<section>


    <table class="styled-table">
        <thead>
        <tr>
                    <th>#</th>
                    <th>Name</th>
                    <th>Price</th>
                    <th>24h%</th>
        </tr>
        </thead>
        <tbody>
        <% currencies.forEach( el => { %>

        <tr class="active-row">
            <td><%= el.market_cap_rank %></td>-->
                    <td>
                        <img class="coinImages" src="<%= el.image %>" alt="">
                        <%= el.name %>

                    </td>
                    <td><%= el.current_price %></td>
                    <td><%= el.price_change_percentage_24h %></td>
        </tr>
        <% }) %>
        </tbody>
    </table>
</section>

<script type="text/javascript" src="js/main.js"></script>
</body>
</html>
